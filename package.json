{
    "name": "smart-brain-server",
    "version": "1.0.0",
    "engines": {
        "node": "10.16.0",
        "npm": "6.9.0"
    },
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "build": "cd client && npm run build",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "node server.js"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "nodemon": "^1.19.1"
    },
    "devDependencies": {
        "concurrently": "^4.1.1"
    }
}
